\n\n\nfrom selenium import webdriver\nfrom selenium.webdriver.common.keys import Keys\nimport time\n\n# Set up the Chrome driver\ndriver = webdriver.Chrome()\n\n# Navigate to the Admin UI page for the gateway\ngateway_ip = '192.168.0.1:8080'\ndriver.get(gateway_ip)\n\n# Enter valid admin credentials (username and password) when prompted\nusername = 'admin'\npassword = 'password'\nusername_input = driver.find_element_by_name('username')\npassword_input = driver.find_element_by_name('password')\nusername_input.send_keys(username)\npassword_input.send_keys(password)\npassword_input.send_keys(Keys.RETURN)\n\n# Wait for the system to authenticate the user\ntime.sleep(5)\n\n# Click on the 'System Settings' tab in the Admin UI dashboard\nsystem_settings = driver.find_element_by_xpath('//a[@href='/admin/settings']')\nsystem_settings.click()\n\n# Look for and click on the option labeled as 'Factory Reset' or 'Advanced Settings' followed by 'Factory Reset'\nfactory_reset = driver.find_element_by_xpath('//a[contains(text(),'Factory Reset')]')\nfactory_reset.click()\n\n# Confirm the reset by clicking 'Yes' in the pop-up dialog box\nyes_button = driver.find_element_by_xpath('//button[@class='btn btn-primary']')\nyes_button.click()\n\n# Wait for the gateway to complete the factory reset process, which might take a few minutes\ntime.sleep(5 * 60)  # 5 minutes\n\n# Once the reset is complete, the Admin UI page will be renewed with default settings\nassert driver.find_element_by_xpath('//h1[text()='Gateway Setup']')\nprint('Factory reset completed successfully!')\n\n# Close the browser window\ndriver.quit()