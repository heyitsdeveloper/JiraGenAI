\n\nfrom selenium import webdriver\nfrom selenium.webdriver.common.by import By\nimport time\n\n# Initialize the Chrome driver\ndriver = webdriver.Chrome()\n\n# Navigate to the Admin UI URL\nadmin_ui_url = 'http://192.168.0.1/admin'\ndriver.get(admin_ui_url)\n\n# Log in to the Admin UI with valid credentials\nusername = driver.find_element(By.NAME, 'username')\npassword = driver.find_element(By.NAME, 'password')\nlogin_button = driver.find_element(By.XPATH, '//button[@type='submit']')\n\nusername.send_keys('admin')\npassword.send_keys('strong_password')\nlogin_button.click()\n\n# Click on the Factory Reset option or menu item\nfactory_reset_button = driver.find_element(By.LINK_TEXT, 'Factory Reset')\nfactory_reset_button.click()\n\n# Confirm the reset action by clicking 'Yes' or 'OK' when prompted for confirmation\nconfirmation_dialog = driver.switch_to.alert\nconfirmation_dialog.accept()\n\n# Wait for the factory reset process to complete and observe any confirmation messages or status indicators\ntime.sleep(120)  # Factory reset may take several minutes, adjust as needed\n\n# Verify that all settings have been reset to their default values by checking configuration pages in the Admin UI\ndefault_values = {\n    'Network': {'IP Address': '192.168.0.1', 'Subnet Mask': '255.255.255.0'},\n    'Security': {'SSH Access': False, 'HTTPS': False},\n    # Add more default values as needed...\n}\nnetwork_page = driver.find_element(By.LINK_TEXT, 'Network')\nnetwork_page.click()\nip_address = driver.find_element(By.NAME, 'ipAddress')\nsubnet_mask = driver.find_element(By.NAME, 'subnetMask')\n\nassert ip_address.get_attribute('value') == default_values['Network']['IP Address']\nassert subnet_mask.get_attribute('value') == default_values['Network']['Subnet Mask']\n\n# Close the driver when finished\ndriver.quit()