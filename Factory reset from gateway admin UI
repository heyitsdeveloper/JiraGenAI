\n\n\nfrom selenium import webdriver\nfrom selenium.common.exceptions import NoSuchElementException, TimeoutException\nfrom selenium.webdriver.common.keys import Keys\nfrom selenium.webdriver.support import expected_conditions as EC\nfrom selenium.webdriver.support import ui as UI\nfrom selenium.webdriver.common.by import By\nfrom time import sleep\n\ndef login_gateway_admin_ui(driver, username, password):\n    driver.get('http://<gateway_ip>:<gateway_port>/admin/')\n    sleep(5)\n    driver.find_element_by_name('username').send_keys(username)\n    driver.find_element_by_name('password').send_keys(password)\n    driver.find_element_by_tag_name('button').click()\n    sleep(5)\n\ndef navigate_to_settings_page(driver):\n    driver.find_element_by_link_text('Settings').click()\n    sleep(5)\n\ndef click_on_factory_reset_option(driver):\n    driver.find_element_by_xpath('//a[contains(text(),'Factory reset'))]]').click()\n    sleep(5)\n\ndef confirm_factory_reset_action(driver):\n    driver.find_element_by_xpath('//button[contains(text(),'Yes') or contains(text(),'Confirm'))]]').click()\n    sleep(5)\n\ndef logout_gateway_admin_ui(driver):\n    driver.find_element_by_tag_name('button').click()\n    sleep(5)\n    driver.quit()